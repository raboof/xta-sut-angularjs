{
  "name": "xta-sut-angular",
  "private": true,
  "version": "0.0.0",
  "description": "XTA System Under Test for AngularJS",
  "dependencies": {},
  "devDependencies": {
    "karma": "^0.12.16",
    "karma-jasmine": "^0.1.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "protractor": "~1.0.0",
    "http-server": "^0.6.1",
    "tmp": "0.0.23",
    "bower": "^1.3.1"
  },
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "http-server -a 0.0.0.0 -p 8000 -c-1",
    "pretest": "npm install",
    "test": "node node_modules/karma/bin/karma start karma.conf.js",
    "test-single-run": "node node_modules/karma/bin/karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e/protractor.conf.js"
  }
}
